        });\r\n        case RES_PHOTOS:\r\n            return Object.assign({}, state, {\r\n                isFetching: false,\r\n                albums: action.albums\r\n            });\r\n        case REQ_TEST:\r\n            return Object.assign({}, state, {\r\n                isFetching: true\r\n            });\r\n        case RES_TEST:\r\n            return Object.assign({}, state, {\r\n                isFetching: false,\r\n                test: action.test\r\n            });\r\n        case GEN_ERR:\r\n            return Object.assign({}, state, {\r\n                isFetching: false,\r\n                error: action.error,\r\n            });\r\n        case CLEAR_ERR:\r\n            return Object.assign({}, state, {\r\n                error: action.error,\r\n            });\r\n        default:\r\n            return state;\r\n    }\r\n}\r\nvar reducer = combineReducers({\r\n    all: reduce\r\n});\r\nvar store = createStore(reducer, composeWithDevTools(applyMiddleware(thunkMiddleware)));\r\nfunction photos() {\r\n    ReactDOM.render(React.createElement(Provider, { store: store },\r\n        React.createElement(Photos, null)), document.getElementById('root'));\r\n}\r\nfunction blinds() {\r\n    ReactDOM.render(React.createElement(Blinds, null), document.getElementById('blinds'));\r\n}\r\nfunction photoGallery() {\r\n    ReactDOM.render(React.createElement(PhotoGallery, null), document.getElementById('photoGallery'));\r\n}\r\nexport { store, populate, photos, blinds, photoGallery };\r\n//# sourceMappingURL=index.js.map","map":"{\"version\":3,\"file\":\"index.js\",\"sourceRoot\":\"\",\"sources\":[\"../src/public/index.tsx\"],\"names\":[],\"mappings\":\";AACA,eAAe;AACf,OAAO,KAAK,KAAK,MAAM,OAAO,CAAC;AAC/B,OAAO,KAAK,QAAQ,MAAM,WAAW,CAAC;AACtC,OAAO,EAAE,QAAQ,EAAE,MAAM,aAAa,CAAC;AACvC,OAAO,EAAE,mBAAmB,EAAE,MAAM,0BAA0B,CAAC;AAC/D,OAAO,eAAe,MAAM,aAAa,CAAC;AAC1C,OAAO,EAAE,WAAW,EAAE,eAAe,EAAE,eAAe,EAAE,MAAM,OAAO,CAAC;AAEtE,aAAa;AACb,OAAO,MAAM,MAAM,UAAU,CAAA;AAC7B,OAAO,MAAM,